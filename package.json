{
  "name": "collection",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "serve": "vite --mode development",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build --mode production",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "prettier-format": "prettier \"src/**/*.{vue,js,ts}\" --write",
    "lint:css": "stylelint \"**/*.{vue,css,sass,scss,less}\" --fix",
    "lint:all": "pnpm run lint && pnpm run prettier-format && pnpm run lint:css",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{vue,less,css,sass,scss}": [
      "stylelint --fix"
    ],
    "src/**/*.{js,vue,ts}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.0.10",
    "element-plus": "^2.2.25",
    "pinia": "^2.0.26",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@iconify-json/ep": "^1.1.8",
    "@iconify-json/logos": "^1.1.19",
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/node": "^18.11.9",
    "@unocss/preset-attributify": "^0.47.3",
    "@unocss/preset-uno": "^0.47.3",
    "@unocss/transformer-directives": "^0.47.3",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.19",
    "postcss-html": "^1.5.0",
    "prettier": "^2.7.1",
    "sass": "^1.56.1",
    "stylelint": "^14.15.0",
    "stylelint-config-recommended-scss": "^8.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^29.0.0",
    "typescript": "~4.7.4",
    "unocss": "^0.47.3",
    "unplugin-auto-import": "^0.12.0",
    "unplugin-icons": "^0.14.14",
    "unplugin-vue-components": "^0.22.11",
    "vite": "^3.2.4",
    "vue-tsc": "^1.0.9"
  }
}
