{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "sourceMap": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "preserveValueImports": false,
    "lib": [
      "ESNext",
      "DOM"
    ],
    "skipLibCheck": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "/@/*": [
        "src/*"
      ],
      "/cpns/*": [
        "src/components/*"
      ]
    },
    "types": [
      "vite/client",
      "element-plus/global"
    ]
  },
  "include": [
    "src/**/*",
    "src/**/*.vue",
    "src/**/*.d.ts",
    "./**/*.d.ts",
  ],
  "references": [
    {
      "path": "./tsconfig.config.json"
    }
  ]
}